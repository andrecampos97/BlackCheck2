<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="blackcheck.com.br">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.EmpresaActivity"></activity>
        <activity android:name=".activity.RegistroActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.AutenticacaoActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>